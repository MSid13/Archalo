{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/archalo",
        "destination": "/Archalo.html"
      },
      {
        "source": "/laws",
        "destination": "/laws.html"
      },
      {
        "source": "/artifactHelp",
        "destination": "/artifactHelp.html"
      },
      {
        "source": "/edu-resources",
        "destination": "/edu-resources.html"
      },
      {
        "source": "/credits",
        "destination": "/credits.html"
      },
      {
        "source": "/",
        "destination": "/Archalo.html"
      }
    ],
    "redirects": [
      {
        "source": "/index.html",
        "destination": "/Archalo.html",
        "type": 301
      },
      {
        "source": "/feedback",
        "destination": "https://forms.gle/QsAZcC3v9v6xbyw79",
        "type": 301
      },
      {
        "source": "/guide",
